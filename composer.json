{
  "name": "jurager/exchange",
  "type": "library",
  "description": "Catalogues exchange between 1C (CommerceML) and Laravel",
  "keywords": [
    "1C",
    "CommerceML",
    "Laravel"
  ],
  "homepage": "https://github.com/jurager",
  "license": "MIT",
  "authors": [
    {
      "name": "Yuri Gerassimov",
      "email": "jurager01@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Jurager\\Exchange\\": "src/"
    }
  },
  "require": {
    "ext-iconv": "*",
    "ext-zip": "*",
    "php": "^8.0",
    "jurager/commerce": "^1.0",
    "illuminate/contracts": "^8.0|^9.0",
    "illuminate/support": "^8.0|^9.0",
    "illuminate/http": "^8.0|^9.0",
    "illuminate/routing": "^8.0|^9.0",
    "symfony/http-foundation": "^5.0|^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jurager\\Exchange\\ExchangeServiceProvider"
      ]
    }
  }
}
